config_version=5

[application]

config/name="Arcane Raiders"
run/main_scene="res://debug/test_room.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Game="*res://managers/game_manager.gd"
MultiplayerInput="*res://addons/multiplayer_input/multiplayer_input.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_position_type=2

[editor_plugins]

enabled=PackedStringArray("res://addons/multiplayer_input/plugin.cfg")

[file_customization]

folder_colors={
"res://elements/": "orange",
"res://managers/": "red",
"res://moving_entities/": "teal",
"res://moving_entities/player/": "green",
"res://spells/": "purple"
}

