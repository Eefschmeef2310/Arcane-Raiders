script/source = "extends Area2D
#class_name
#Authored by AlexV. Please consult for any modifications or major feature requests.

#region Variables
	#Signals

	#Enums

	#Constants

	#Exported Variables
	#@export_group(\"Group\")
	#@export_subgroup(\"Subgroup\")
@export var field_damage: int = 0
@export var max_puddles : int = 10

	#Onready Variables
@onready var animation_player = $AnimationPlayer

	#Other Variables (please try to separate and organise!)
var base_damage
var resource 
var caster 
var infliction_time = 1
#endregion

#region Godot methods
func _ready():
	base_damage = field_damage
	
	#Impose limit
	if get_tree().get_nodes_in_group(get_groups()[0]).size() > max_puddles:
		get_tree().get_last_node_in_group(get_groups()[0]).animation_player.play(\"kill\")
#endregion

#region Signal methods

#endregion

#region Other methods (please try to separate and organise!)

#endregion

func _on_timer_timeout():
	for moving_entity in get_overlapping_bodies():
		if moving_entity.is_in_group(\"player\") and moving_entity != caster:
			moving_entity.on_hurt(self)


func _on_kill_timer_timeout():
	$AnimationPlayer.play(\"kill\")
